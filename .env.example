# Gemini Ultra Gateway - Environment Configuration

# ============== Server Settings ==============
HOST=0.0.0.0
PORT=5000
DEBUG=false

# ============== Database ==============
# SQLite (default)
DATABASE_URL=sqlite:///./geminibusiness.db

# PostgreSQL (optional)
# DATABASE_URL=postgresql://user:password@localhost:5432/gemini_gateway

# ============== Security ==============
SECRET_KEY=change-this-to-a-secure-random-string
ADMIN_USERNAME=admin
ADMIN_PASSWORD=admin123456

# ============== Proxy (Optional) ==============
# HTTP/HTTPS proxy for connecting to Google
# PROXY=http://127.0.0.1:7890

# ============== Timeout Settings ==============
TIMEOUT_SECONDS=600
JWT_TTL_SECONDS=270

# ============== Account Cooldown ==============
ACCOUNT_COOLDOWN_SECONDS=300
AUTH_ERROR_COOLDOWN_SECONDS=900
RATE_LIMIT_COOLDOWN_SECONDS=300

# ============== Multi-Account Configuration ==============
# Account 1
ACCOUNT1_NAME=account-1
ACCOUNT1_SECURE_C_SES=your_secure_c_ses_value
ACCOUNT1_CSESIDX=your_csesidx_value
ACCOUNT1_CONFIG_ID=your_config_id_value
ACCOUNT1_HOST_C_OSES=your_host_c_oses_value

# Account 2 (optional)
# ACCOUNT2_NAME=account-2
# ACCOUNT2_SECURE_C_SES=...
# ACCOUNT2_CSESIDX=...
# ACCOUNT2_CONFIG_ID=...
# ACCOUNT2_HOST_C_OSES=...

# ============== Legacy Single Account (Alternative) ==============
# If you only have one account, you can use these instead:
# SECURE_C_SES=your_secure_c_ses_value
# CSESIDX=your_csesidx_value
# CONFIG_ID=your_config_id_value
# HOST_C_OSES=your_host_c_oses_value

# ============== Auto Login Settings ==============
# Enable automatic cookie refresh
AUTO_LOGIN_ENABLED=false
TEMPMAIL_WORKER_URL=

# ============== Keep-Alive Settings ==============
KEEPALIVE_ENABLED=true
KEEPALIVE_INTERVAL_MINUTES=30

# ============== Media Cache ==============
IMAGE_CACHE_HOURS=24
VIDEO_CACHE_HOURS=24
